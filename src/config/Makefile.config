###############################################################################
##  All configurable variables are defined in the file named Makefile.config.in
##  When Autoconf is run, it will create a file named Makefile.config which
##  will have all of the configurable variables replaced with their values.
###############################################################################



srcdir = .
top_srcdir = ..
prefix = /home/ulrikey/hypre-dist/hypre-2.10.1-130-g7586c02
exec_prefix = ${prefix}



HYPRE_RELEASE_NAME    = hypre
HYPRE_RELEASE_VERSION = 2.10.1
HYPRE_RELEASE_DATE    = 2015/09/11
HYPRE_RELEASE_TIME    = 00:00:00
HYPRE_RELEASE_BUGS    = hypre-support@llnl.gov

HYPRE_SRC_TOP_DIR = /home/ulrikey/hypre-dist/hypre-2.10.1-130-g7586c02/src
HYPRE_BUILD_DIR   = /home/ulrikey/hypre-dist/hypre-2.10.1-130-g7586c02/src/hypre

HYPRE_INSTALL_DIR = /home/ulrikey/hypre-dist/hypre-2.10.1-130-g7586c02
HYPRE_LIB_INSTALL = ${exec_prefix}/lib
HYPRE_INC_INSTALL = ${prefix}/include

HYPRE_LIB_SUFFIX = .a

.SUFFIXES:
.SUFFIXES: .o .f .c .C .cxx .cc

.f.o:
	$(FC) $(FFLAGS) -c $< 
.c.o:
	$(CC) $(CFLAGS) -c $< 
.C.o:
	$(CXX) $(CXXFLAGS) -c $<
.cxx.o:
	$(CXX) $(CXXFLAGS) -c $<
.cc.o:
	$(CXX) $(CXXFLAGS) -c $<

FC       = mpif77
FFLAGS    = -O2 -g -O2 $(FC_COMPILE_FLAGS)

CC        = mpicc
CFLAGS    = -O2 -DHAVE_CONFIG_H $(C_COMPILE_FLAGS)

CXX       = mpicxx
CXXFLAGS  = -O2 -DHAVE_CONFIG_H $(CXX_COMPILE_FLAGS)

LINK_FC  = mpif77
LINK_CC   = mpicc
LINK_CXX  = mpicxx

BUILD_FC_SHARED    = 
BUILD_CC_SHARED     = 
BUILD_CXX_SHARED    = 
SHARED_COMPILE_FLAG = 
SHARED_BUILD_FLAG   = 
SHARED_SET_SONAME   = 
SHARED_OPTIONS      = 

BUILD_PYTHON = 0
PYTHON       = 

BUILD_JAVA = 0

AR     = ar -rcu
RANLIB = ranlib

LDFLAGS = 
LIBS    =    -lstdc++ -lm
FLIBS   = 


touch mup_fake_ext.h
cat internal_protos.h > mup_fake_int.h

../config/gen_code.sh mup_fake_ext.h mup_fake_int.h onlydef

rm -f mup_fake_ext.h
rm -f mup_fake_int.h

# Replace MUP_FAKE_INT
sed 's/MUP_FAKE_INT/DISTRIBUTED_MATRIX/' mup_fake_int_mup_def.h > _hypre_distributed_matrix_mup_def.h
rm -f mup_fake_int_mup_def.h
